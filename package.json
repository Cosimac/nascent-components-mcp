{
  "name": "@jzone-mcp/antd-components-mcp",
  "version": "1.0.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/server.js",
  "description": "一个支持 Ant Design 组件库信息查询的 MCP 服务",
  "bin": "dist/server.js",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "tsup && tsx test-server.ts",
    "extract": "tsx scripts/extract-docs.ts ../ant-design",
    "inspector": "pnpm build & npx @modelcontextprotocol/inspector node dist/server.js",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "mcp",
    "antd",
    "ant design",
    "model context protocol",
    "ui components"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "to-vfile": "^8.0.0",
    "vfile-matter": "^5.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
